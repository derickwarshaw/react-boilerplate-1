{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "description": "Apptension's React Boilerplate",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Apptension",
  "license": "MIT",
  "scripts": {
    "precommit": "npm run lint:eslint .",
    "analyze:clean": "rimraf stats.json",
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "extract-intl": "babel-node --presets latest,stage-0 -- ./internals/scripts/extract-intl.js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress && npm run ssr:generate-production",
    "build:clean": "npm run test:clean && rimraf ./dist/*",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development SHOW_VERSION=true VERSION=0.0.1 npm-run-all --parallel ssr:generate:watch ssr:start start:dev",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:dev": "cross-env NODE_ENV=development node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "presetup": "npm i chalk shelljs",
    "setup": "node ./internals/scripts/setup.js",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "lint:watch": "esw app --color -w",
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register internals/testing/test.index.spec.js --recursive \"./app/**/*.spec.js\"",
    "test:clean": "rimraf ./coverage",
    "test:watch": "npm run test -- --watch --no-single-run",
    "test:junit": "npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=./junit/junitOutput.xml",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha --compilers js:babel-core/register internals/testing/test.index.spec.js --recursive \"app/**/*.spec.js\" ",
    "ssr:generate": "cross-env NODE_ENV=development webpack --progress --config internals/webpack/webpack.ssr.babel.js",
    "ssr:generate-production": "cross-env NODE_ENV=production webpack --progress --config internals/webpack/webpack.ssr.babel.js",
    "ssr:generate:watch": "npm run ssr:generate -- --watch",
    "ssr:start": "cross-env NODE_ENV=development nodemon -q --ext js,json --watch server/middlewares --watch server/generated --watch server/devRenderService.js server/devRenderService.js",
    "ssr:clean": "rimraf server/middlewares/generated.*"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "npm-run-all",
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css",
      "nodemon",
      "nodemailer",
      "multiparty"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "nyc": {
    "include": [
      "app/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "html",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "chalk": "2.1.0",
    "compression": "1.7.0",
    "cross-env": "5.0.5",
    "express": "^4.15.5",
    "express-http-proxy": "^1.0.6",
    "fontfaceobserver": "2.0.13",
    "history": "^4.6.3",
    "htmlescape": "^1.1.1",
    "http-proxy": "^1.16.2",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "ip": "1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.17.4",
    "minimist": "1.2.0",
    "multiparty": "^4.1.3",
    "nodemailer": "^4.1.1",
    "nodemon": "^1.12.1",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-helmet": "5.2.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-intl": "2.4.0",
    "react-redux": "5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "next",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "3.7.2",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "redux-immutable": "4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "0.15.6",
    "reduxsauce": "^0.6.0",
    "reselect": "3.0.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-dynamic-import-node": "1.1.0",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "3.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.8",
    "babel-preset-latest": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "^4.1.0",
    "chai-enzyme": "^0.8.0",
    "cheerio": "1.0.0-rc.2",
    "circular-dependency-plugin": "4.2.1",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.16",
    "css-loader": "^0.28.7",
    "enzyme": "^2.7.1",
    "eslint": "4.7.1",
    "eslint-config-apptension-react": "^0.0.10",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-watch": "^3.1.2",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "0.11.2",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.14.3",
    "image-webpack-loader": "3.4.2",
    "imports-loader": "0.7.1",
    "jsdom": "^11.1.0",
    "lint-staged": "4.2.1",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "ngrok": "2.2.22",
    "node-plop": "0.7.1",
    "node-sass": "^4.5.0",
    "null-loader": "0.1.1",
    "nyc": "^11.1.0",
    "offline-plugin": "4.8.3",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "15.6.0",
    "redux-saga-tester": "^1.0.197",
    "rimraf": "2.6.2",
    "sass-loader": "^6.0.2",
    "shelljs": "0.7.8",
    "sinon": "^3.3.0",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "webpack": "^3.4.1",
    "webpack-appversion-plugin": "^1.2.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-node-externals": "^1.6.0",
    "webpack-spritesmith": "^0.3.1"
  }
}
